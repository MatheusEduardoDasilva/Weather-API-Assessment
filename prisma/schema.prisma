generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Weather {
  id          Int      @id @default(autoincrement())
  cidade      String
  temperatura Float
  descricao   String
  umidade     Int
  criadoEm    DateTime @default(now())
}
